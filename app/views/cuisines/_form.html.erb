<%= form_for @cuisine do |f| %>
  <fieldset class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
    <p class="help-block"><em>
      <%= t(:select_cuisines_example, scope:[:select_prompt])%>
    </em></p>
  </fieldset>

  <%= f.submit class: "btn btn-primary"%>
<% end %>
